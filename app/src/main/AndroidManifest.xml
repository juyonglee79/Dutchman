        <activity android:name=".ui.activity.appintro.AppIntroActivity" />
        <activity android:name=".ui.activity.alertset.AlertSetActivity" />
        <activity android:name=".ui.activity.profileset.ProfileSetActivity" />
        <activity android:name=".ui.activity.main.MainActivity" />
        <activity android:name=".ui.activity.notice.LocationServiceTermsActivity" />
        <activity android:name=".ui.activity.notice.ServiceTermsActivity" />
        <activity android:name=".ui.activity.notice.PrivatePolicyActivity" />
        <activity android:name=".ui.activity.setting.SettingActivity"/>
        <activity android:name=".ui.activity.setting.profilechange.ProfileChangeActivity"/>
        <activity android:name=".ui.activity.setting.emailcertification.EmailCertificationActivity"/>
        <activity android:name=".ui.activity.setting.pwchange.PwChangeActivity"/>

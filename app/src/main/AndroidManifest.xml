        <activity android:name=".ui.activity.setting.opensource.OpenSourceActivity"/>
        <activity android:name=".ui.activity.addpost.AddPostActivity"/>
        <activity android:name=".ui.activity.profileimageset.ProfileImageSetActivity"/>
        <activity android:name=".ui.activity.releaselimitset.ReleaseLimitSetActivity"/>
        <activity android:name=".ui.activity.room.RoomActivity"/>
        <activity android:name=".ui.activity.search.SearchActivity"/>
    </application>

    
</manifest>